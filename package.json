{
  "name": "Todoist",
  "description": "build of Todoist with webview in Electron",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . Todoist --overwrite --platform linux --arch x64 --out dist/",
    "deb64": "electron-installer-debian --src dist/Todoist-linux-x64 --out dist/installers --arch amd64 --config debian.json"
  },
  "license": "MIT",
  "devDependencies": {
    "electron": "^3.0.9",
    "electron-reload": "^1.3.0"
  },
    "author": "Reni Dantas"
}
